    function internalTrade(address _from, uint256 _amount) internal returns(uint256) {
        address _kyberFromRef = _from;
        uint256 _value = 0;

        if (_from == EthAddressLib.ethAddress()) {
            _kyberFromRef = KYBER_ETH_MOCK_ADDRESS;
            _value = _amount;
